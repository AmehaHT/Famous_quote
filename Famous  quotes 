    "# Find the most common tags across all quotes"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "4a3f1336",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1200x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "from wordcloud import WordCloud \n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "tags = df2['Tags'].str.split(', ')\n",
    "\n",
    "# Create frequency count of tags\n",
    "tag_counts = {}\n",
    "for quote_tags in tags:\n",
    "    for tag in quote_tags:\n",
    "        if tag in tag_counts:\n",
    "            tag_counts[tag] += 1\n",
    "        else:\n",
    "            tag_counts[tag] = 1\n",
    "\n",
    "# Create word cloud\n",
    "wordcloud = WordCloud(width=800, height=400, background_color='white').generate_from_frequencies(tag_counts)\n",
    "\n",
    "# Display word cloud\n",
    "plt.figure(figsize=(12,8))\n",
    "plt.imshow(wordcloud, interpolation='bilinear')\n",
    "plt.axis('off')\n",
    "plt.show()"
   ]
  },
  {
