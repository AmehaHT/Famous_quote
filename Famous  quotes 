   "source": [
    "from apyori import apriori\n",
    "from mlxtend.frequent_patterns import apriori, association_rules"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "1ff544cd",
   "metadata": {},
   "outputs": [],
   "source": [
    "tag_lists = sampled_df['Tags'].str.split(',').tolist()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "bc94fcec",
   "metadata": {},
   "outputs": [],
   "source": [
    "from mlxtend.preprocessing import TransactionEncoder\n",
    "\n",
    "te = TransactionEncoder()\n",
    "te_array = te.fit(tag_lists).transform(tag_lists)\n",
    "df_tags = pd.DataFrame(te_array, columns=te.columns_)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "9b74a0e5",
   "metadata": {},
   "outputs": [],
   "source": [
    "frequent_itemsets = apriori(df_tags, min_support=0.01, use_colnames=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "33686de6",
   "metadata": {},
   "outputs": [],
   "source": [
